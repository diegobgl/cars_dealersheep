<odoo>
    <template id="vehicle_snippet" name="Vehicle Snippet">
        <section class="oe_snippet_body">
            <t t-call="vehicle_management.VehicleSnippet"/>
        </section>
    </template>

    <template id="vehicle_snippet_block" name="Vehicle Snippet Block">
        <div class="oe_snippet_thumbnail">
            <img src="/vehicle_management/static/description/snippet_thumbnail.png"/>
            <span>Catálogo de Vehículos</span>
        </div>
    </template>
</odoo>
